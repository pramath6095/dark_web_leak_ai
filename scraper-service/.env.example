# ── Tor proxy ──────────────────────────────────────────────────────────────
TOR_PROXY_HOST=127.0.0.1
TOR_PROXY_PORT=9050

# ── Concurrency ───────────────────────────────────────────────────────────
MAX_WORKERS=3
NUM_ENGINES=17
SCRAPE_LIMIT=20

# ── Inter-service URLs ────────────────────────────────────────────────────
QUERY_SERVICE_URL=http://query-generator:8001
ANALYSIS_SERVICE_URL=http://ai-analysis:8000

# ── Batching ──────────────────────────────────────────────────────────────
BATCH_SIZE=5

# ── Polling ───────────────────────────────────────────────────────────────
POLL_INTERVAL_SECONDS=300
